				"next": "notWash",
				"condition": "true"
			}]
		},
		"notGum": {
			"type": "event",
			"event": {
				"name": "fade out",
				"key": "notGum"
			}
		},
		"wash": {
			"type": "dialog",
			"fragments": [{
				"tag": "Voces",
				"msn": "JaJaJaJaJa"
			},
			{
				"tag": "Voces",
				"msn": "Menuda mancha <$ varObject('global', 'UserNameNonStatic') $>, parece que te hayas cagado encima"
			}]
		},
		"notWash": {
			"type": "dialog",
			"fragments": [{
				"tag": "Voces",
