{
	"mother": {
		"root": {
			"type": "fork",
			"options": [{
				"next": "node2",
				"condition": "spoken == true"
			},
			{
				"next": "init",
				"condition": "true"
			}]
		},
		"init":{
			"type": "setter",
			"next": "initSchool",
			"set": {
				"var": "spoken",
				"value": true
			}
		},
		"initSchool":{
			"type": "setter",
			"next": "initGuille",
			"set": {
				"var": "aboutSchool",
				"value": true
			}
		},
		"initGuille":{
			"type": "setter",
			"next": "initAlison",
			"set": {
				"var": "aboutGuille",
				"value": true
			}
		},
		"initAlison":{
			"type": "setter",
			"next": "initJose",
			"set": {
				"var": "aboutAlison",
				"value": true
			}
		},
