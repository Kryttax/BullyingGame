		},
		"node1": {
			"type": "dialog",
			"next": "setter",
			"fragments": [{
				"tag": "Mama",
				"msn": "Ya has vuelto <$ varObject('global', 'UserNameNonStatic') $>"
			}, 
			{
				"tag": "Mama",
				"msn": "¿Qué tal te ha ido el día?"
			}]
		},
		"node2": {
			"type": "dialog",
			"next": "setter",
			"fragments": [{
